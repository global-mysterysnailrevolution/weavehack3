{
  "mcpServers": {
    "rlm-vla": {
      "command": "python",
      "args": [
        "-m",
        "rvla.mcp_stdio_server"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "OPENAI_API_KEY": "${env:OPENAI_API_KEY}",
        "WANDB_API_KEY": "${env:WANDB_API_KEY}",
        "WANDB_PROJECT": "${env:WANDB_PROJECT}",
        "WANDB_ENTITY": "${env:WANDB_ENTITY}"
      }
    }
  }
}
